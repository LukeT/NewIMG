{
    "rules": {
        "indent": [
            2,
            "tab"
        ],
        "quotes": [
            2,
            "single"
        ],
        "semi": [
            2,
            "always"
        ],
        "no-underscore-dangle": [
            0
        ]
    },
    "env": {
        "es6": true,
        "node": true
    },
    "globals": {

    },
    "ecmaFeatures": {
        modules: true
    },
    "parserOptions": {
        sourceType: "module"
    },
    "extends": "airbnb/base"
}
